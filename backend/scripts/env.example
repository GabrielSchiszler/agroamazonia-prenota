# Exemplo de arquivo .env para teste OAuth2
# Copie este arquivo para .env e preencha com suas credenciais
# Ou use: cp env.example .env

# URL do endpoint OAuth2 do ServiceNow
OCR_FAILURE_AUTH_URL=https://agroamazoniad.service-now.com/oauth_token.do

# Credenciais OAuth2 (obtenha do Postman ou ServiceNow)
OCR_FAILURE_CLIENT_ID=seu_client_id_aqui
OCR_FAILURE_CLIENT_SECRET=seu_client_secret_aqui

# Credenciais de usuário
OCR_FAILURE_USERNAME=fast.ocr
OCR_FAILURE_PASSWORD=sua_password_aqui

# URL da API para reportar falhas (opcional, para teste de chamada)
OCR_FAILURE_API_URL=https://agroamazoniad.service-now.com/api/x_aapas_fast_ocr/ocr/reportar-falha

# ============================================
# Configurações Protheus API
# ============================================

# URL da API Protheus
export PROTHEUS_API_URL=https://api.agroamazonia.com/hom-ocr

# URL do endpoint OAuth2 do Protheus (Token URL)
# Exemplo: https://api.agroamazonia.com/oauth/token
export PROTHEUS_AUTH_URL={{tokenUrl}}

# Credenciais OAuth2 Client Credentials (obtenha do Postman ou Protheus)
export PROTHEUS_CLIENT_ID={{clientId}}
export PROTHEUS_CLIENT_SECRET={{clientSecret}}

# Scope (opcional)
# export PROTHEUS_SCOPE=read:org

